package com.example.nbiservice.model;

import java.util.List;

public class TapiAlarm {
    private String uuid;
    private String eventTime;
    private String severity;
    private List<String> problemDescription;

    public TapiAlarm() {}

    public TapiAlarm(String uuid, String eventTime, String severity, List<String> problemDescription) {
        this.uuid = uuid;
        this.eventTime = eventTime;
        this.severity = severity;
        this.problemDescription = problemDescription;
    }

    public String getUuid() { return uuid; }
    public String getEventTime() { return eventTime; }
    public String getSeverity() { return severity; }
    public List<String> getProblemDescription() { return problemDescription; }
}
